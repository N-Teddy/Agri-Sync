{
    "version": 2,
    "installCommand": "pnpm install",
    "buildCommand": "pnpm run build",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "OPTIONS",
                "DELETE",
                "HEAD"
            ]
        }
    ]
}